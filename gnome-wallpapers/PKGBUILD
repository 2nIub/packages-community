pkgname=gnome-wallpapers
_pkgname=black_leather_manjaro_linux_by_tefrem34.jpg
_repo=art-works
pkgver=20160321
pkgrel=1
_snapshot=59194fd37ce339b1719a3a4a00857ed7a3a72ddc
pkgdesc='Manjaro Linux Gnome wallpapers'
arch=('any')
url="https://github.com/Ste74/$_repo"
license=('GPL')
source=("$_repo-$_snapshot.tar.gz::$url/archive/$_snapshot.tar.gz")
sha256sums=('f201fefc9e05f401f3eaa01ca8f8ed7671aa34726a0d3338e0caf1e7dd8ccd79')

pkgver() {
	date +%Y%m%d
}

package(){
	install -d $pkgdir/usr/share/backgrounds
	install $srcdir/${_pkgname} $pkgdir/usr/share/backgrounds
}

