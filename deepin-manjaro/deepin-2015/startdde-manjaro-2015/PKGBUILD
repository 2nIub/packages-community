pkgname=startdde-manjaro-2015
pkgver=2.92.0
_commit=69761f6d59f754d4c7f166283f29fd385bb39c18
pkgrel=1
pkgdesc="starter of deepin desktop environment"
arch=('i686' 'x86_64')
url="https://github.com/linuxdeepin/startdde"
license=('GPL3')
makedepends=('cmake'
	'coffee-script'
	'deepin-daemon'
	'deepin-dbus-factory'
	'deepin-wm'
	'go-packages'
	'python2'
	'webkitgtk')
conflicts=('startdde' 'startdde-manjaro')
source=("git+https://github.com/linuxdeepin/startdde.git#commit=$_commit")
sha256sums=('SKIP')

build() {
	cd startdde-${pkgver}
	make GOPATH="/usr/lib/go"
}

package() {
	cd startdde-${pkgver}
	make GOPATH="/usr/lib/go" DESTDIR="${pkgdir}" install
}
