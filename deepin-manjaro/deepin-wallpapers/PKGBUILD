_profile=deepin
_user=oberon2007
_snapshot=e7f5288c0fdf3b5f7eeeaeeb319d68f3ca042940

pkgname="$_profile-wallpapers"
pkgver=20160126
pkgrel=1
pkgdesc="Wallpapers for $_profile"
arch=any
license=GPL3
url=https://github.com
groups=('deepin-manjaro')
source="$pkgname-$pkgver.tar.gz::$url/$_user/$pkgname/archive/$_snapshot.tar.gz"

md5sums=('4f8ad9189994c27bb628a29e62327288')

package() {
	cd $srcdir/$pkgname-$_snapshot
	install -dm755 $pkgdir/usr/share
	cp -r * $pkgdir/usr/share

}
