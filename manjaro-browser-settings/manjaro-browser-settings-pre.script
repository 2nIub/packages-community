#!/bin/bash

#Used folder

firefox_dir=/usr/lib/firefox/distribution
firefox_dev_dir=/usr/lib/firefox-developer-edition

##Remove distribution.ini

#Check if exist firefox
if [[ -f "$firefox_dir"/distribution.ini ]]; then
	rm -f "$firefox_dir"/distribution.ini
fi

#Check if exist firefox-developer
if [[ -f "$firefox_dev_dir"/distribution.ini ]]; then
	rm -f "$firefox_dev_dir"/distribution.ini
fi
