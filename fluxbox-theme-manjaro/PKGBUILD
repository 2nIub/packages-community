pkgname=fluxbox-theme-manjaro
pkgver=20150829
pkgrel=1
pkgdesc="theme for manjaro-fluxbox"
arch=('any')
license=('GPL')
depends=('fluxbox')
source="theme.cfg"
md5sums=('9a8423a8f2be40a324987e9981b3ce35')
package() {
	cd "${srcdir}"
	install -Dm644 "${srcdir}/theme.cfg" "${pkgdir}/usr/share/fluxbox/styles/manjaro-flux/theme.cfg"
}
